#include <stdio.h>

int a, b;

void findplayer ()
{
		if (a == 1)
		{
			if (b == 1)
			{
				printf ("1x1");	
			}	
			
			else if (b == 2)
			{
				printf ("1x2");
			}
			
			else if (b == 3)
			{
				printf ("1x3");
			}
			
			else if (b == 4)
			{
				printf ("1x4");
			}
			
			else if (b == 5)
			{
				printf ("1x5");
			}
			
			else if (b == 6)
			{
				printf ("1x6");
			}
			
			else 
			{
				printf ("b is not a number btween 1 and 6");
			}
		}
		
